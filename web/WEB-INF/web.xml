<?xml version="1.0" encoding="UTF-8" ?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/javaee/web-app_3_1.xsd"
         version="3.1">
    <servlet>
        <servlet-name>GetJsonFromSQL</servlet-name>
        <servlet-class>Servlets.GetJsonFromSQL</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>GetJsonFromSQL</servlet-name>
        <url-pattern>/getJSONfromSQL</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>LoadJSONtoMySQL</servlet-name>
        <servlet-class>Servlets.LoadJSONtoMySQL</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoadJSONtoMySQL</servlet-name>
        <url-pattern>/LoadJSON</url-pattern>
    </servlet-mapping>
</web-app>